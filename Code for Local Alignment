
s="atgc"
t="ggct"
mat=2
mm=2
l=[[0 for j in range(len(t)+1)]for i in range(len(s)+1)]
print(l)
for i in range(1,len(s)+1):
  for j in range(1,len(t)+1):
    if(s[i-1]==t[j-1]):
      l[i][j]=l[i-1][j-1]+mat
    else:
      l[i][j]=max(0,l[i-1][j-1]-mm,l[i-1][j]-mm,l[i][j-1]-mm)
for i in range(len(s)+1):
  for j in range(len(t)+1):
    print(l[i][j],end='\t')
  print()
