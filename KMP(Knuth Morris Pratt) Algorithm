#PI TABLE

P="ABAABC"
i=1
j=0
f[0]=0
f=[0 for i in range(6)]
while i<len(P):
  if P[j]==P[i]:
    j=j+1
    f[i]=j
    i=i+1
  else:
    if j>0:
      f[j-1]=f[i]
      i+=1
    else:
      f[i]=0
      i=i+1
print(f)

#Matching!!
T="ABBABAABCABB"
n=len(T)
m=len(P)
i,j=0,0
while i<n:
 if P[j]==T[i]:
  i+=1
  j+=1
  if j==m:
    print("Pattern at ",i-j)
    j=0
 else:
  if j!=0:
    j=f[j-1]
    j=0
  else:
    i+=1
